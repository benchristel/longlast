{
    "name": "@longlast",
    "entryPoints": ["../../pkg/*/src/index.ts"],
    "plugin": ["typedoc-plugin-markdown"],
    "entryFileName": "index",
    "hidePageHeader": true,
    "hideBreadcrumbs": true,
    "parametersFormat": "table",
    "expandParameters": true,
    // We set `excludeExternals` to keep TypeDoc from linking to source files
    // in node_modules when the type of an export comes from another package
    // (e.g. the `Curried2` type, which is the type of `export const equals`).
    "excludeExternals": true
}
